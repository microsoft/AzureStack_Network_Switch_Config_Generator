
! Name: S31R28-TOR2
! Make: Cisco
! Model: 93180YC-FX


hostname S31R28-TOR2


! vlan.go.tmpl-define_vlan
    
vlan 2
  name Unused
    
vlan 7
  name Tenant0
    
vlan 8
  name Management
    
vlan 25
  name BMCMgmt
    
vlan 15
  name Tenant1
    
vlan 711
  name Storage1
    
vlan 712
  name Storage2




! vlan.go.tmpl-interface_vlan
    
interface vlan2
  description Unused
  mtu 9000
  shutdown
    
interface vlan7
  description Tenant0
  mtu 9000
  no shutdown
  ip address 10.10.100.3/24
    
interface vlan8
  description Management
  mtu 9000
  no shutdown
  ip address 10.10.101.3/24
    
interface vlan25
  description BMCMgmt
  mtu 9000
  no shutdown
  ip address 192.168.0.3/26
    
interface vlan15
  description Tenant1
  mtu 9000
  no shutdown
  ip address 10.100.0.3/24
    
interface vlan711
  description Storage1
  mtu 9000
  no shutdown
  
    
interface vlan712
  description Storage2
  mtu 9000
  no shutdown
  




! vlan.go.tmpl-oob_vlan



interface vlan25
  description BMCMgmt
  mtu 9000
  no shutdown
  ip address 192.168.0.3/26






! port.go.tmpl-port
    
interface TenGigEthernet 1/1
  description Host Port
  no cdp enable
  mtu 9000
  switchport
  switchport mode trunk
  switchport trunk native vlan 8
  switchport trunk allowed vlan add 0
  no logging event port link-status
  spanning-tree port type edge trunk
  priority-flow-control mode on
  no ip redirects
  no ipv6 redirects
  no shutdown 
    
interface TenGigEthernet 1/2
  description Unused
  no cdp enable
  mtu 9000
  switchport
  switchport access vlan 2
  no logging event port link-status
  no ip redirects
  no ipv6 redirects
  shutdown 
    
interface TenGigEthernet 1/3
  description Unused
  no cdp enable
  mtu 9000
  switchport
  switchport access vlan 2
  no logging event port link-status
  no ip redirects
  no ipv6 redirects
  shutdown 
    
interface TenGigEthernet 1/4
  description Unused
  no cdp enable
  mtu 9000
  switchport
  switchport access vlan 2
  no logging event port link-status
  no ip redirects
  no ipv6 redirects
  shutdown 
    
interface TenGigEthernet 1/16
  description Storage Port
  no cdp enable
  mtu 9000
  switchport
  switchport mode trunk
  switchport trunk native vlan 8
  switchport trunk allowed vlan add 711
  switchport trunk allowed vlan add 712
  no logging event port link-status
  spanning-tree port type edge trunk
  priority-flow-control mode on
  no ip redirects
  no ipv6 redirects
  no shutdown 
    
interface TenGigEthernet 1/46
  description BMCMgmt
  no cdp enable
  mtu 9000
  switchport
  switchport access vlan 25
  no logging event port link-status
  no ip redirects
  no ipv6 redirects
  no shutdown 
    
interface TenGigEthernet 1/48
  description P2P_TOR2_To_Border1/TOR2
  no cdp enable
  mtu 9000
  no switchport
  ip address 192.168.1.4/31
  no ip redirects
  no ipv6 redirects
  no shutdown 




! bgp.go.tmpl-bgp
router bgp 65000
  router-id 192.168.1.9/32
  bestpath as-path multipath-relax
  log-neighbor-changes
  address-family ipv4 unicast
    maximum-paths 8
    network 10.100.0.0/24
    network Compute
    network 10.10.101.0/24
    network 192.168.1.0/28
    network 192.168.0.0/26

  neighbor 192.168.1.5/31
    remote-as 63000
    description P2P_TOR2_To_Border1/Border1
    address-family ipv4 unicast
      maximum-prefix 12000 warning-only
  