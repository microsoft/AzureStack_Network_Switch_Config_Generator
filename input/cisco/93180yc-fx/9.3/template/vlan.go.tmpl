{{ define "define_vlan"}}
{{range .}}    
vlan {{.VlanID}}
  name {{.VlanName}}
{{end}}
{{ end }}

{{ define "interface_vlan"}}
{{range .}}    
interface vlan{{.VlanID}}
  description {{.VlanName}}
  mtu {{.Mtu}}
  {{if .Shutdown -}} 
  shutdown 
  {{else -}} 
  no shutdown
  {{if ne .IPAddress "" -}} ip address {{ .IPAddress }}{{end}}
  {{- end}}
{{end}}
{{ end }}